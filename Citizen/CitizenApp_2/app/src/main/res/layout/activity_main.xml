<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    android:paddingTop="25dp">

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="600dp"
        >

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:layout_marginRight="5dp"
            android:layout_marginBottom="8dp">

            <Button
                android:layout_width="100dp"
                android:layout_height="75dp"
                android:onClick="traffic"
                android:id="@+id/gog_traffic"
                android:background="@drawable/traffic"
                android:layout_weight="1"
                android:layout_gravity="center"
                android:layout_marginLeft="16dp"
                android:layout_marginTop="16dp"
                android:layout_marginRight="16dp"
                android:layout_marginBottom="8dp" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="Live Traffic"
                android:id="@+id/textView5"
                android:layout_below="@+id/linearLayout2"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:textSize="15sp"
                android:layout_weight="1"
                android:gravity="center"
                android:layout_marginBottom="10dp" />

            <Button
                android:layout_width="100dp"
                android:layout_height="75dp"
                android:background="@drawable/sos"
                android:id="@+id/sos"

                android:layout_weight="1"
                android:layout_gravity="center"
                android:text="sos"


                android:layout_marginLeft="16dp"
                android:layout_marginRight="16dp"
                android:layout_marginBottom="16dp"
                android:layout_marginTop="5dp" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="SOS"
                android:id="@+id/textView7"
                android:layout_below="@+id/linearLayout3"
                android:layout_alignLeft="@+id/linearLayout3"
                android:layout_alignStart="@+id/linearLayout3"
                android:gravity="center"
                android:textSize="15sp" />

            <Button
                android:layout_width="100dp"
                android:layout_height="75dp"
                android:background="@drawable/pin_drop"
                android:id="@+id/nearby"
                android:layout_weight="1"
                android:layout_gravity="center"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="10dp"
                android:layout_marginRight="10dp" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="Near by Places"
                android:id="@+id/textView9"
                android:layout_weight="1"
                android:gravity="center"
                android:textSize="15sp"
                android:layout_marginTop="5dp" />

        </LinearLayout>

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true"
            android:layout_weight="1"
            android:layout_marginBottom="8dp">

            <Button
                android:layout_width="100dp"
                android:layout_height="75dp"
                android:background="@drawable/complaint"
                android:id="@+id/Quick"
                android:layout_weight="1"
                android:layout_margin="16dp"
                android:layout_gravity="center"
                android:layout_marginLeft="16dp" />

            <TextView
                android:id="@+id/textView6"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_below="@+id/linearLayout"
                android:layout_weight="1"
                android:gravity="center"
                android:text="Quick Report"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:textSize="15sp" />

            <Button
                android:layout_width="100dp"
                android:layout_height="75dp"
                android:background="@drawable/complaint"
                android:id="@+id/women"
                android:layout_weight="1"
                android:layout_margin="16dp"
                android:layout_gravity="center" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="Women Complaint"
                android:id="@+id/textView8"
                android:layout_below="@+id/linearLayout3"
                android:layout_toRightOf="@+id/textView7"
                android:layout_toEndOf="@+id/textView7"
                android:gravity="center"
                android:textSize="15sp" />

            <Button
                android:layout_width="100dp"
                android:layout_height="75dp"
                android:background="@drawable/rate"
                android:id="@+id/area"
                android:layout_weight="1"
                android:layout_margin="10dp"
                android:layout_gravity="center" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/latlng"
                android:visibility="gone"
                android:layout_weight="1" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="Rate your Area"
                android:id="@+id/textView10"
                android:layout_weight="1"
                android:gravity="center"
                android:textSize="15sp" />

        </LinearLayout>

    </LinearLayout>

    <!-- <LinearLayout
         android:orientation="horizontal"
         android:layout_width="match_parent"
         android:layout_height="wrap_content"

         android:layout_alignParentBottom="true"
         android:id="@+id/linearLayout4"
         android:background="#006064">

         <Button
             android:layout_width="match_parent"
             android:layout_height="wrap_content"
             android:text="nearby Places"
             android:id="@+id/nearby"
             android:textColor="#FFFFFF"
             android:background="#006064"
             android:layout_weight="1"
             android:textSize="15sp" />
     </LinearLayout>

 -->
</RelativeLayout>